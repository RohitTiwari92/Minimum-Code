        <TextBox x:Name="lns" HorizontalAlignment="Left" Height="23" Margin="274,179,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="lns_TextChanged"/>
        <Label x:Name="pro" Content="processing....." HorizontalAlignment="Left" Margin="102,152,0,0" VerticalAlignment="Top" Width="135"/>
